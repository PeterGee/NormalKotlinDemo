    <declare-styleable name="FolderTextView">
        <attr name="foldText" format="string" />
        <attr name="unFoldText" format="string" />
        <attr name="foldLine" format="integer" />
        <attr name="tailTextColor" format="color" />
        <attr name="canFoldAgain" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CapsuleIndicatorProgress">
        <attr name="changeByProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DailyPlanNumberProgress">
        <attr name="progress_textSize" format="dimension" />
        <attr name="progress_textPadding" format="dimension" />
        <attr name="color_type" format="enum" >
            <enum name="dark_blue" value="0"/>
            <enum name="orange" value="1"/>
            <enum name="light_blue" value="2"/>
        </attr>
    </declare-styleable>
</resources>